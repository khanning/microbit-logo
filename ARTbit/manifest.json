{
  "manifest_version": 2,
  "name": "art:bit",
  "version": "0.93",
  "minimum_chrome_version": "23",
  "permissions": ["serial", "storage", {"fileSystem": ["write", "retainEntries", "directory"]}],
  "icons": {
    "16": "16_16.png",
    "128": "128_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"],
      "persistent": false
    }
  }
}
